{
  "name": "my-blog",
  "version": "0.2.0",
  "license": "MIT",
  "author": "Cody Brunner <rockchalkwushock@icloud.com> (https://github.com/rockchalkwushock)",
  "homepage": "https://rcws-development.com/",
  "description": "My blog built with React & Next.",
  "repository": "https://github.com/rockchalkwushock/my-blog.git",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "commit": "git cz",
    "dev": "next",
    "lint": "eslint components layout lib pages --fix",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --print-width 80 --trailing-comma all --no-semi --use-tabs",
    "start": "NODE_ENV=production node index.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "now": {
    "name": "rcws-blog",
    "env": {
      "DISQUS": "@disqus",
      "GATRACKINGID": "@blog_ga_tracking_id",
      "NODE_ENV": "production"
    },
    "files": [
      ".babelrc",
      "components",
      "env.config.js",
      "lib",
      "pages",
      "static",
      "index.js",
      "package.json",
      "posts.json",
      "yarn.lock"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "yarn prettier",
      "git add"
    ]
  },
  "dependencies": {
    "babel-plugin-transform-define": "~1.3.0",
    "next": "^2.4.7",
    "nprogress": "~0.2.0",
    "prop-types": "~15.5.10",
    "react": "^15.6.1",
    "react-disqus-thread": "~0.4.0",
    "react-dom": "^15.6.1",
    "react-fontawesome": "~1.6.1",
    "react-ga": "~2.2.0",
    "react-highlight": "~0.10.0",
    "react-youtube": "~7.4.0",
    "redux": "^3.7.1"
  },
  "devDependencies": {
    "cz-conventional-changelog": "~2.0.0",
    "eslint": "^4.2.0",
    "eslint-config-equimper": "^2.2.1",
    "eslint-config-prettier": "^2.3.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.1",
    "prettier": "^1.5.2"
  }
}
